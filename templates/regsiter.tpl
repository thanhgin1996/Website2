<script>
    (function() {
        const imageText = document.querySelector('.center-text');

        // Giá trị Captcha từ server (mã PHP sẽ chèn tại đây)
        const captchaText = "<?php echo $newcaptcha; ?>";

        // Hàm kiểm tra DevTools
        function detectDevTools() {
            const threshold = 160; // Ngưỡng kích thước để nhận biết DevTools mở
            const isDevToolsOpen = window.outerWidth - window.innerWidth > threshold || 
                                   window.outerHeight - window.innerHeight > threshold;

            if (isDevToolsOpen) {
                // Nếu mở DevTools, thay đổi style
                if (imageText) {
                    imageText.style.color = 'transparent'; // Ẩn text
                    imageText.style.textShadow = 'none'; // Loại bỏ shadow nếu có
                    imageText.innerText = ''; // Xóa nội dung text
                }
            } else {
                // Nếu đóng DevTools, khôi phục giá trị ban đầu
                if (imageText) {
                    imageText.style.color = ''; // Khôi phục màu chữ
                    imageText.style.textShadow = ''; // Khôi phục shadow nếu cần
                    imageText.innerText = captchaText; // Đặt lại giá trị Captcha
                }
            }
        }

        // Lắng nghe sự kiện resize để phát hiện
        window.addEventListener('resize', detectDevTools);

        // Kiểm tra trạng thái DevTools ngay khi tải trang
        detectDevTools();
    })();
</script>


